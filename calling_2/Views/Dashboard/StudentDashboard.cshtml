@using Newtonsoft.Json

@{
    ViewBag.Title = "Student Dashboard";
}

<h2>Student Dashboard</h2>

@if (ViewBag.StudentData != null)
{
    <div>
        <h3>Student Details</h3>
        <!-- Render student dashboard data here -->
        <pre>@JsonConvert.SerializeObject(ViewBag.StudentData)</pre>
    </div>
}
else
{
    <div>@ViewBag.ErrorMessage</div>
}
